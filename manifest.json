{
  "manifest_version": 3,
  "name": "dankmemes â€” Password Manager",
  "permissions": [
    "webNavigation",
    "activeTab",
    "storage",
    "tabs",
    "windows"
  ],
  "description": "Base Level Extension",
  "version": "1.0",
  "background": {
      "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "js": ["src/main.js"],
      "css": ["src/main.css"],
      "matches": ["http://*/*", "https://*/*"]
    } 
  ],
  "action": {
    "default_popup": "login/extension.html",
    "default_icon": {
      "16": "pictures/LOGO.png",
      "32": "pictures/LOGO.png",
      "48": "pictures/LOGO.png",
      "128": "pictures/LOGO.png"
    }
  }  
}